{
  "name": "emoji-mart",
  "version": "1.0.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:figma/emoji-mart.git"
  },
  "files": [
    "/dist",
    "/packages/emoji-mart-data/i18n",
    "/packages/emoji-mart-data/sets"
  ],
  "type": "module",
  "source": "packages/emoji-mart/src/index.js",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "includeNodeModules": true
    }
  },
  "scripts": {
    "build": "parcel build --no-autoinstall",
    "build:data": "cd packages/emoji-mart-data && node --experimental-json-modules ./build.js",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.3.2",
    "@parcel/transformer-sass": "2.3.2",
    "autoprefixer": "10.4.4",
    "emoji-datasource": "14.0.0",
    "emojilib": "3.0.6",
    "inflection": "1.13.2",
    "jest": "28.0.0",
    "jest-environment-jsdom": "28.0.0",
    "parcel": "2.3.2",
    "prettier": "2.5.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "unicode-emoji-json": "0.3.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "engines": {
    "yarn": ">=1"
  }
}
