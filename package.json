{
  "name": "@emoji-mart/monorepo",
  "version": "1.0.2",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:figma/emoji-mart.git"
  },
  "files": [
    "packages/emoji-mart-data/i18n/*",
    "packages/emoji-mart-data/sets/*"
  ],
  "type": "module",
  "source": "./packages/emoji-mart/src/index.js",
  "scripts": {
    "build": "parcel build --no-autoinstall",
    "build:data": "cd packages/emoji-mart-data && node --experimental-json-modules ./build.js",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.3.2",
    "@parcel/transformer-sass": "2.3.2",
    "autoprefixer": "10.4.4",
    "emoji-datasource": "14.0.0",
    "emojilib": "3.0.6",
    "inflection": "1.13.2",
    "jest": "28.0.0",
    "jest-environment-jsdom": "28.0.0",
    "parcel": "2.3.2",
    "preact": "10.6.4",
    "prettier": "2.5.1",
    "unicode-emoji-json": "0.3.1"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "engines": {
    "yarn": ">=1"
  }
}
